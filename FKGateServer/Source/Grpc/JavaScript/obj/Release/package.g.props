<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <PackageJsonName Condition="$(PackageJsonName) == ''">flicker-grpc-server</PackageJsonName>
    <PackageJsonVersion Condition="$(PackageJsonVersion) == ''">1.0.0</PackageJsonVersion>
    <PackageJsonDescription Condition="$(PackageJsonDescription) == ''">Flicker应用的gRPC服务，包括邮箱验证码和密码加密服务</PackageJsonDescription>
    <PackageJsonMain Condition="$(PackageJsonMain) == ''">index.js</PackageJsonMain>
    <PackageJsonScriptsStart Condition="$(PackageJsonScriptsStart) == ''">cross-env NODE_ENV=production node index.js</PackageJsonScriptsStart>
    <PackageJsonScriptsStartVerification Condition="$(PackageJsonScriptsStartVerification) == ''">cross-env NODE_ENV=production node ./server/verification.js</PackageJsonScriptsStartVerification>
    <PackageJsonScriptsStartEncryption Condition="$(PackageJsonScriptsStartEncryption) == ''">cross-env NODE_ENV=production node ./server/encryption.js</PackageJsonScriptsStartEncryption>
    <PackageJsonScriptsStartAuthentication Condition="$(PackageJsonScriptsStartAuthentication) == ''">cross-env NODE_ENV=production node ./server/authentication.js</PackageJsonScriptsStartAuthentication>
    <PackageJsonScriptsDev Condition="$(PackageJsonScriptsDev) == ''">cross-env NODE_ENV=development nodemon index.js</PackageJsonScriptsDev>
    <PackageJsonScriptsDevVerification Condition="$(PackageJsonScriptsDevVerification) == ''">cross-env NODE_ENV=development nodemon ./server/verification.js</PackageJsonScriptsDevVerification>
    <PackageJsonScriptsDevEncryption Condition="$(PackageJsonScriptsDevEncryption) == ''">cross-env NODE_ENV=development nodemon ./server/encryption.js</PackageJsonScriptsDevEncryption>
    <PackageJsonScriptsDevAuthentication Condition="$(PackageJsonScriptsDevAuthentication) == ''">cross-env NODE_ENV=development nodemon ./server/authentication.js</PackageJsonScriptsDevAuthentication>
    <PackageJsonScriptsDevAll Condition="$(PackageJsonScriptsDevAll) == ''">cross-env NODE_ENV=development concurrently "npm run dev:verification" "npm run dev:encryption" "npm run dev:authentication"</PackageJsonScriptsDevAll>
    <PackageJsonScriptsDebug Condition="$(PackageJsonScriptsDebug) == ''">cross-env NODE_ENV=development node --inspect index.js</PackageJsonScriptsDebug>
    <PackageJsonScriptsDebugVerification Condition="$(PackageJsonScriptsDebugVerification) == ''">cross-env NODE_ENV=development node --inspect ./server/verification.js</PackageJsonScriptsDebugVerification>
    <PackageJsonScriptsDebugEncryption Condition="$(PackageJsonScriptsDebugEncryption) == ''">cross-env NODE_ENV=development node --inspect ./server/encryption.js</PackageJsonScriptsDebugEncryption>
    <PackageJsonScriptsDebugAuthentication Condition="$(PackageJsonScriptsDebugAuthentication) == ''">cross-env NODE_ENV=development node --inspect ./server/authentication.js</PackageJsonScriptsDebugAuthentication>
    <PackageJsonDependenciesGrpcGrpcJs Condition="$(PackageJsonDependenciesGrpcGrpcJs) == ''">^1.13.4</PackageJsonDependenciesGrpcGrpcJs>
    <PackageJsonDependenciesGrpcProtoLoader Condition="$(PackageJsonDependenciesGrpcProtoLoader) == ''">^0.7.15</PackageJsonDependenciesGrpcProtoLoader>
    <PackageJsonDependenciesYuuangFfiRsWin32X64Msvc Condition="$(PackageJsonDependenciesYuuangFfiRsWin32X64Msvc) == ''">^1.3.0</PackageJsonDependenciesYuuangFfiRsWin32X64Msvc>
    <PackageJsonDependenciesBcrypt Condition="$(PackageJsonDependenciesBcrypt) == ''">^6.0.0</PackageJsonDependenciesBcrypt>
    <PackageJsonDependenciesFfiRs Condition="$(PackageJsonDependenciesFfiRs) == ''">^1.3.0</PackageJsonDependenciesFfiRs>
    <PackageJsonDependenciesIoredis Condition="$(PackageJsonDependenciesIoredis) == ''">^5.6.1</PackageJsonDependenciesIoredis>
    <PackageJsonDependenciesNodemailer Condition="$(PackageJsonDependenciesNodemailer) == ''">^7.0.3</PackageJsonDependenciesNodemailer>
    <PackageJsonDependenciesUuid Condition="$(PackageJsonDependenciesUuid) == ''">^11.1.0</PackageJsonDependenciesUuid>
    <PackageJsonDevdependenciesConcurrently Condition="$(PackageJsonDevdependenciesConcurrently) == ''">^8.2.2</PackageJsonDevdependenciesConcurrently>
    <PackageJsonDevdependenciesCrossEnv Condition="$(PackageJsonDevdependenciesCrossEnv) == ''">^7.0.3</PackageJsonDevdependenciesCrossEnv>
    <PackageJsonDevdependenciesEslint Condition="$(PackageJsonDevdependenciesEslint) == ''">^9.31.0</PackageJsonDevdependenciesEslint>
    <PackageJsonDevdependenciesNodemon Condition="$(PackageJsonDevdependenciesNodemon) == ''">^3.0.1</PackageJsonDevdependenciesNodemon>
    <PackageJsonAuthor Condition="$(PackageJsonAuthor) == ''"></PackageJsonAuthor>
    <PackageJsonLicense Condition="$(PackageJsonLicense) == ''">ISC</PackageJsonLicense>
  </PropertyGroup>
</Project>